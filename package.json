{
  "name": "gfc",
  "version": "0.0.0",
  "angular-cli": {},
  "scripts": {
    "build:versioned": "npm run version && npm run build",
    "build:versioned:dev": "npm run version && npm run build:dev",
    "build": "ng build -prod --build-optimizer",
    "build:dev": "ng build -dev",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "serve": "ng serve --proxy-config proxy.conf.json",
    "serve:hmr": "ng serve --host 0.0.0.0 --disable-host-check --proxy-config proxy.conf.json --hmr -e=hmr --",
    "start": "npm run serve:hmr",
    "test": "npm run version && ng test --watch=false",
    "version": "git log -1 --date=format:\"%y.%m.%d %H:%M:%S\" --pretty=format:\"export const gfcVersion = '%h / %cd';\" > \"./src/app/app.version.ts\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.3.6",
    "@angular/cdk": "2.0.0-beta.10",
    "@angular/common": "4.3.6",
    "@angular/compiler": "4.3.6",
    "@angular/core": "4.3.6",
    "@angular/forms": "4.3.6",
    "@angular/http": "4.3.6",
    "@angular/material": "2.0.0-beta.10",
    "@angular/platform-browser": "4.3.6",
    "@angular/platform-browser-dynamic": "4.3.6",
    "@angular/router": "4.3.6",
    "@angular/tsc-wrapped": "^4.3.1",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.1",
    "hammerjs": "^2.0.8",
    "moment": "^2.18.1",
    "ng2-cookies": "^1.0.12",
    "ng2-toasty": "^4.0.3",
    "normalize.css": "^7.0.0",
    "raven-js": "^3.17.0",
    "rxjs": "^5.4.3",
    "ts-helpers": "^1.1.1",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.17"
  },
  "devDependencies": {
    "@angular/cli": "1.4.1",
    "@angular/compiler-cli": "4.3.6",
    "@angularclass/hmr": "^2.1.3",
    "@types/google.analytics": "^0.0.35",
    "@types/jasmine": "^2.5.53",
    "@types/node": "^8.0.26",
    "codelyzer": "^3.1.2",
    "jasmine-core": "^2.6.4",
    "jasmine-spec-reporter": "^4.1.1",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.0.2",
    "protractor": "^5.1.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^2.5.2"
  }
}
